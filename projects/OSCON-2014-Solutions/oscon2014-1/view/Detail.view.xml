<mvc:View
	controllerName="oscon2014.view.Detail"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc" >
	<Page
		title="{i18n>DetailTitle}"
		showNavButton="true"
		navButtonPress="handleNavButtonPress" >
		<VBox>
			<Text text="{SoId}" />
			<Text text="{GrossAmount}" />
			<Text text="{CreatedAt}" />
			<Text text="{CreatedByBp}" />
		</VBox>
	</Page>
</mvc:View>
